Description: Create new phoen record
Endpoint: http://localhost:3002/api/v1/Phone/create-phone
Method: POST
Params: none
request payload: 
{
	
	"brand": "Android",
	"model": "Galaxy S8",
	"color": "Coral Blue"
}
Respose: 
{
    "message": "Success",
    "resp": {
        "data": {
            "id": 3,
            "brand": "Android",
            "model": "Galaxy S8",
            "color": "Coral Blue",
            "user_id": null,
            "active": true,
            "created_by": "lphan",
            "updated_by": null,
            "created_at": "2020-05-09T22:12:35.531Z",
            "updated_at": "2020-05-09T22:12:35.531Z",
            "deleted_at": null
        },
        "statusCode": 200
    }
}

